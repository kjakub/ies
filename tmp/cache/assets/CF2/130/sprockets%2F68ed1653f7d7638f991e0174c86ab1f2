FI"o'use strict';

var myApp = angular.module('myApp',['ngResource','ui'])

myApp.config(function ($routeProvider, $httpProvider) {
    $routeProvider.
        when('/', 
          { templateUrl: '/assets/welcome.html' }).
        
        when('/notes/', 
          {templateUrl: '/assets/notes/index.html', controller: 'NotesController'}).

        when('/notes/:note_id', 
          {templateUrl: '/assets/notes/show.html', controller: 'NotesShowController'}).

        // when('/photographers/:photographer_id/galleries',
        //   {template: '/assets/galleries.html', controller: GalleriesCtrl}).       

        // when('/photographers/:photographer_id/galleries/:gallery_id/photos',
        //   {template: '/assets/photos.html', controller: PhotosCtrl}).
        
        otherwise({ redirectTo: '/' });

    $httpProvider.defaults.headers.post['Content-Type'] = 'application/json'
    $httpProvider.defaults.headers.put['Content-Type'] = 'application/json'
    //assumes the presence of jQuery
    var token = $("meta[name='csrf-token']").attr("content");
    $httpProvider.defaults.headers.common['X-CSRF-Token'] = token;
});
